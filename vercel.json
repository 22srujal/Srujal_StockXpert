{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "auth.html",
      "use": "@vercel/static"
    },
    {
      "src": "auth.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/auth.html",
      "dest": "/auth.html"
    },
    {
      "src": "/auth.css",
      "dest": "/auth.css"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
